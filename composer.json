{
    "name": "klipper/config",
    "type": "library",
    "description": "Config component of Klipper",
    "keywords": ["Config", "Loader", "Annotation"],
    "homepage": "https://github.com/klipperdev/config",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@klipper.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4.0|^8.0.0",
        "doctrine/annotations": "^1.6.0",
        "symfony/config": "^5.2.0",
        "symfony/finder": "^5.2.0"
    },
    "require-dev": {
        "klipper/doctrine-extra": "^2.0.0",
        "symfony/http-kernel": "^5.2.0"
    },
    "autoload": {
        "psr-4": { "Klipper\\Component\\Config\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
